package knightarcherandfootman;

public class Troop {
	protected String atackType;
	protected int speed;
	protected int ID;
	public static int count = 0;

	public void Troop() {
		ID=count;
		count++;
		System.out.println("Troop number " + ID + " spawned.");
	}

	public int getID() {
		return ID;
	}

}
