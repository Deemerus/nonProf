import java.io.File;
import java.io.FileNotFoundException;
import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;

public class App {

	public static void main(String[] args) {
		Map<Integer, Node> nodes = new HashMap<Integer, Node>();
		try(Scanner scanner = new Scanner(new File("graph2"))){
			while(scanner.hasNextLine()){
				System.out.println(scanner.nextInt() + " " + scanner.nextInt());
				System.out.println(scanner.nextInt());
				int id = scanner.nextInt()*1000+scanner.nextInt();
				nodes.put(id, new Node());
				while(scanner.hasNextInt()){
					int nid=scanner.nextInt()*1000+scanner.nextInt();
					nodes.get(id).addNeighbour(new Neighbour(nid, scanner.nextInt()));
				}
				scanner.nextLine();
			}
		} catch (FileNotFoundException e) {
			System.out.println("File graph2 not found.");
		}
		nodes.get(23141).printNeighbours();
		

	}
}
