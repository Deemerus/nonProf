import java.util.ArrayList;
import java.util.List;

public class Node {
	private int height;
	private int length;
	List<Neighbour> neighbours = new ArrayList<Neighbour>();
	
	Node(int height, int length) {
		this.height = height;
		this.length = length;
	}

	public int getHeight() {
		return height;
	}

	public int getLength() {
		return length;
	}
	
	public void addNeighbour(x, y, distance);
}
