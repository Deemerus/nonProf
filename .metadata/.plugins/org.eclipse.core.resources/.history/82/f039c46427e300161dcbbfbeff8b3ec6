
public class Node {
	private int height;
	private int length;
	private int[] leftNeighbour = new int[3];
	private int[] rightNeighbour = new int[3];
	private int[] upNeighbour = new int[3];
	private int[] downNeighbour = new int[3];

	Node(int height, int length) {
		this.height = height;
		this.length = length;
	}

	public int getHeight() {
		return height;
	}

	public int getLength() {
		return length;
	}
}
