package knightarcherandfootman;

public class Pikeman extends Troop {
	public static int pikemanCount = 0;
	
	public Pikeman() {
		atackType = "melee";
		pikemanID = pikemanCount;
		count++;
		pikemanCount++;
		System.out.println("Pikeman number " + ID + " spawned and has " + atackType + " atack type.");
		
	}

}
